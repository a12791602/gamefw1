
.panel-header {
    background-image: url(../images/header-bg.png);
    background-repeat: repeat-x;
    position: relative;
}

.hideme{
    display: none;
}
.clearfix:after,.clearfix:after{
	content:"";
	display: block;
}
.clearfix:after {
	clear:both;
}
.clearfix {
	*zoom: 1;
}

.user-info {
	color: #fff;
}

.user-info li {
	float: left;
}

.user-info .ui-wrap ul {
	overflow: hidden;
	margin-bottom: 10px;
}
.user-info .ui-wrap ul:last-child {
	margin-bottom: 0px;
}
.user-info .ui-wrap .c1 {
	font-size: 15px;
}
.user-info .ui-wrap .c1 li {
    margin-right: 30px;	
}
.user-info .ui-wrap .c2 label {
    font-size: 35px;
    display: inline-block;
    position: relative;
    top: -4px;
}
.user-info .ui-wrap .c2  {
    font-size: 55px;
}
.security-tips > * {
	float: left;
	font-size: 14px;
}
.panel-header h6 {
	margin-right: 10px;
}
.user-info .ui-links li {
	float: none;
} 
.user-info .ui-links h3 {
    font-size: 30px;
}
.user-info .ui-links li.timer {
    font-size: 15px;
    padding-left: 26px;
    position: relative;
    margin: 20px 0px 15px 0px;
}
.user-info .ui-links li.timer:before {
    content: "";
    display: inline-block;
    background: url(../images/timer.png);
    position: absolute;
    left: 0px;
    top: -6px;
    width: 24px;
    height: 26px;
}
.user-info .ui-links .nav-timer {
	display:inline-block;
}
.user-info a {
	color: #fff;
}
.user-info .back-home,
.user-info .promo-link {
    border-radius: 5px;
    border: 2px solid #fff;
    padding: 6px 6px;
    display: inline-block;
}
.user-info .promo-link {
    border-color: #eaa103;
    color: #eaa103;
    padding: 5px 17px;
}
.user-info {
    position: relative;
    padding: 40px 0px;
}
.user-info .ui-wrap {
    margin-top: 15px;
    margin-left: 10px;
}
.user-info .ui-links {
    margin-top: 13px;
}
.promo-nav-bg {
    background: url(../images/banner-bg.png);
    position: absolute;
    width: 100%;
    bottom: -40px;
    height: 67px;
    background-position: center center;
    z-index: 11;
}
.panel-container {
    background: url(../images/bg-repeator.jpg);
}
.panel-container .inner{
    position: relative;
    border: 20px solid #1f1f1f;
    box-shadow: 1px 0px 50px #000;
}
.panel-header .inner {
    position: relative;
}
.promo-nav {
    width: 1000px;
    position: absolute;
    bottom: -11px;
    color: #fff;
    height: 36px;
    z-index: 13;
}
.promo-notice-con,
.promo-slider-con {
    width: 45%;	
}
.promo-notice-con,
.promo-slider-con {
    padding-left: 27px;
    position: relative;
}
.promo-notice-con:before,
.promo-slider-con:before {
    display: block;
    content: "";
    width: 27px;
    height: 26px;
    background: url(../images/promo-icon.png);
    position: absolute;
    top: 3px;
    left: 0px;
}
.slide-notice .bd {
    padding: 4px 0 0 20px;
    width: auto;
}
.promo-nav-wrap {
    width: 90%;
    margin: 0 auto;
}
.slide-notice {
	width: 100%;
}
.promo-slider-con li {
	list-style-type: none;
}
.promo-notice-con:before {
    background: url(../images/notice-icon.png);
}
.panel-sidebar .item:hover, .panel-sidebar .on {
    background: url(../images/nav-link-bg.png);
    background-size: 100%;
    width: inherit;
    text-shadow: none;
    text-align: left;
}
.panel-sidebar .item {
    background: url(../images/nav-link-bg-normal.png) repeat;
    background-size: 100%;
    color: #fff;
    background-color: #3b454f;
    text-indent: 110px;
    background-color: transparent;
}
.panel-sidebar {
    padding: 0px 10px 0px 0px;
    background-color: #1f1f1f;
    box-sizing: border-box;
}
.panel-sidebar .title {
    background: url(../images/nav-link-bg.png);
    text-shadow: none;
    background-size: 100%;
    height: 62px;
